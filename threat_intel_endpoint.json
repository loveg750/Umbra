[
  {
    "id": "END-TI-001",
    "name": "Known Malware Hash (Simulated)",
    "description": "Process file hash matches a simulated known malware sample for training purposes.",
    "severity": "critical",
    "log_type": "endpoint",
    "conditions": [
      {
        "field": "file_hash_sha256",
        "operator": "equals",
        "value": "1111111111111111111111111111111111111111111111111111111111111111"
      }
    ],
    "mitre": ["T1204", "T1059"],
    "tags": ["threat_intel", "malware", "hash_indicator"]
  },
  {
    "id": "END-TI-002",
    "name": "Suspicious LOLBin Usage",
    "description": "Living-off-the-land binary used from an unusual directory.",
    "severity": "high",
    "log_type": "endpoint",
    "conditions": [
      {
        "field": "process_name",
        "operator": "in",
        "value": ["powershell.exe", "mshta.exe", "rundll32.exe"]
      },
      {
        "field": "process_path",
        "operator": "not_starts_with",
        "value": "C:\\Windows\\System32"
      }
    ],
    "mitre": ["T1218"],
    "tags": ["threat_intel", "lolbins"]
  },
  {
    "id": "END-TI-003",
    "name": "Persistence via Registry Run Key",
    "description": "Process establishes persistence by writing to a Run or RunOnce registry key (simulated intel pattern).",
    "severity": "high",
    "log_type": "endpoint",
    "conditions": [
      {
        "field": "registry_path",
        "operator": "contains",
        "value": "\\Software\\Microsoft\\Windows\\CurrentVersion\\Run"
      }
    ],
    "mitre": ["T1060", "T1547"],
    "tags": ["threat_intel", "persistence"]
  }
]
