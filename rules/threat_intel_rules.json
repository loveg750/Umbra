[
  {
    "id": "TI-HIGH-SEVERITY-THREAT",
    "name": "High-severity threat intelligence entry",
    "description": "Upstream threat intelligence source marks this entry as high severity. Umbra treats this as a strong external risk signal.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      { "field": "Severity Score", "operator": "greater_than", "value": 3 }
    ],
    "tags": ["threat_intel", "high_risk", "dialect"],
    "mitre": []
  },
  {
    "id": "TI-BREACH-WITH-PASSWORDS",
    "name": "Data breach exposing passwords",
    "description": "Breach entry includes passwords among the exposed data classes, greatly increasing impact and account takeover risk.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "DataClasses", "operator": "contains", "value": "Passwords" }
    ],
    "tags": ["threat_intel", "credential_leak", "hibp", "dialect"],
    "mitre": ["T1589", "T1552"]
  },
  {
    "id": "TI-MASSIVE-BREACH",
    "name": "Massive breach with high record count",
    "description": "Breach affecting more than 10 million records. Umbra treats this as a population-scale incident with long-lasting risk.",
    "severity": "critical",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "PwnCount", "operator": "greater_than", "value": 10000000 }
    ],
    "tags": ["threat_intel", "large_scale", "hibp", "dialect"],
    "mitre": ["T1589"]
  },
  {
    "id": "TI-DDOS-CATEGORY",
    "name": "DDoS threat category entry",
    "description": "Threat intel row categorized as DDoS. Umbra flags this as service availability risk.",
    "severity": "medium",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      { "field": "Threat Category", "operator": "equals", "value": "DDoS" }
    ],
    "tags": ["threat_intel", "ddos", "dialect"],
    "mitre": ["T1499"]
  },

  {
    "id": "TI-PREDICTED-RANSOMWARE-OR-MALWARE-01",
    "name": "Model-predicted ransomware or malware",
    "description": "Dataset prediction engine classifies this entry as ransomware or malware. Umbra treats this like a psychic hunch backed by math.",
    "severity": "critical",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "Predicted Threat Category",
        "operator": "in",
        "value": ["Ransomware", "Malware"]
      }
    ],
    "tags": ["threat_intel", "prediction", "ransomware", "malware", "dialect"],
    "mitre": ["T1486", "T1204"]
  },
  {
    "id": "TI-HIGH-RISK-SCORE-PREDICTION-01",
    "name": "High risk level prediction",
    "description": "Risk Level Prediction from the dataset indicates this threat is in the highest risk band.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      { "field": "Risk Level Prediction", "operator": "greater_than", "value": 3.5 }
    ],
    "tags": ["threat_intel", "dataset_prediction", "dialect"],
    "mitre": ["T1589"]
  },
  {
    "id": "TI-FORUM-SENTIMENT-HYSTERIA-01",
    "name": "High negative sentiment in underground forums",
    "description": "Underground or OSINT sentiment around this threat is extremely elevated, indicating active discussion or exploitation.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      { "field": "Sentiment in Forums", "operator": "greater_than", "value": 0.9 }
    ],
    "tags": ["threat_intel", "osint", "sentiment", "dialect"],
    "mitre": ["T1598"]
  },
  {
    "id": "TI-THREAT-ACTOR-APT-CLUSTER-01",
    "name": "Known APT threat actor involved",
    "description": "Threat intel row attributes this activity to a known high-capability actor such as APT-28 or Lazarus Group.",
    "severity": "critical",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "Threat Actor",
        "operator": "in",
        "value": ["APT-28", "Lazarus Group", "FIN7", "UNC2452"]
      }
    ],
    "tags": ["threat_intel", "apt", "actor", "dialect"],
    "mitre": ["T1589", "T1580"]
  },
  {
    "id": "TI-HIGH-RISK-GEO-01",
    "name": "High-risk geopolitical origin",
    "description": "Threat intel row is associated with a high-risk geopolitical region with a history of offensive operations.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "Geographical Location",
        "operator": "in",
        "value": ["North Korea", "Russia", "Iran", "China"]
      }
    ],
    "tags": ["threat_intel", "geo", "dialect"],
    "mitre": ["T1589"]
  },
  {
    "id": "TI-IOC-MALWARE-ARTIFACTS-01",
    "name": "IOC list contains malware artifacts",
    "description": "Indicators list includes file hashes, executables, or malicious domains typically associated with malware campaigns.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "IOCs (Indicators of Compromise)",
        "operator": "contains",
        "value": ".exe"
      }
    ],
    "tags": ["threat_intel", "ioc", "malware", "dialect"],
    "mitre": ["T1059", "T1204"]
  },
  {
    "id": "TI-SUGGESTED-DEFENSE-HIGH-URGENCY-01",
    "name": "Threat requires active defense change",
    "description": "Suggested defense mechanism calls for configuration, patching, or architectural changes rather than simple monitoring.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "Suggested Defense Mechanism",
        "operator": "contains",
        "value": "Patch"
      }
    ],
    "tags": ["threat_intel", "defense", "dialect"],
    "mitre": ["T1601", "T1600"]
  },
  {
    "id": "TI-LABELED-MALWARE-TOPIC-01",
    "name": "Topic model tagged as malware-focused",
    "description": "Topic modeling labels for this threat entry indicate a malware-centric topic (e.g., payloads, loaders, infections).",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "threat_ioc_kaggle",
    "conditions": [
      {
        "field": "Topic Modeling Labels",
        "operator": "contains",
        "value": "Malware"
      }
    ],
    "tags": ["threat_intel", "topic_model", "dialect"],
    "mitre": ["T1059"]
  },
  {
    "id": "TI-BREACH-SENSITIVE-FLAGGED-01",
    "name": "Sensitive breach flagged by HIBP",
    "description": "Breach is explicitly marked as sensitive and involves user credentials or identity data.",
    "severity": "critical",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "IsSensitive", "operator": "equals", "value": true },
      { "field": "DataClasses", "operator": "contains", "value": "Email addresses" }
    ],
    "tags": ["threat_intel", "hibp", "sensitive", "dialect"],
    "mitre": ["T1589"]
  },
  {
    "id": "TI-UNVERIFIED-PASSWORD-BREACH-01",
    "name": "Unverified breach with passwords exposed",
    "description": "Breach is marked as unverified but includes passwords, which still poses high risk if the data is real.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "IsVerified", "operator": "equals", "value": false },
      { "field": "DataClasses", "operator": "contains", "value": "Passwords" }
    ],
    "tags": ["threat_intel", "hibp", "unverified", "credential_leak", "dialect"],
    "mitre": ["T1589", "T1552"]
  },
  {
    "id": "TI-SPAMLIST-ONLY-LOW-RISK-01",
    "name": "Spam-list style breach with low intrinsic risk",
    "description": "Breach is primarily a spam list (email-only), without passwords or sensitive data, treated as low signal noise.",
    "severity": "low",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "IsSpamList", "operator": "equals", "value": true },
      { "field": "DataClasses", "operator": "not_contains", "value": "Passwords" }
    ],
    "tags": ["threat_intel", "hibp", "spamlist", "noise", "dialect"],
    "mitre": []
  },
  {
    "id": "TI-MALWARE-BREACH-FLAG-01",
    "name": "Breach associated with malware distribution",
    "description": "Breach dataset is flagged as malware-related, indicating it may be feeding infostealer or loader campaigns.",
    "severity": "high",
    "log_type": "threat_intel",
    "dialect": "hibp_breach_csv",
    "conditions": [
      { "field": "IsMalware", "operator": "equals", "value": true }
    ],
    "tags": ["threat_intel", "hibp", "malware", "dialect"],
    "mitre": ["T1555", "T1552"]
  },

